{
  "name": "github-pr-reviewer",
  "version": "1.0.0",
  "description": "A powerful GitHub code review bot that provides automated code analysis",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn src/index.ts",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@octokit/rest": "^19.0.0",
    "@octokit/webhooks": "^10.0.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.1",
    "js-yaml": "^4.1.0",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.2",
    "ts-node-dev": "^1.1.8"
  },
  "author": "Tomas Jankauskas",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tomas-Jankauskas/github-pr-reviewer.git"
  },
  "keywords": [
    "github",
    "code-review",
    "automation",
    "pull-requests",
    "static-analysis"
  ]
}